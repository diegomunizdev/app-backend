    "definitions": {
        "User": {
            "type": "object",
            "required": [
                "id",
                "created_at",
                "username",
                "email",
                "password",
                "cpf",
                "type"
            ],
            "properties": {
                "id": {
                    "readOnly": true,
                    "type": "string",
                    "format": "uuid",
                    "example": "507f191e810c19729de860ea"
                },
                "created_at": {
                    "readOnly": true,
                    "type": "string",
                    "format": "date-time",
                    "description": "Date automatically generated in UTC format according to ISO 8601 at the time the resource is saved.",
                    "example": "2020-08-05T10:00:30Z"
                },
                "username": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "cpf": {
                    "type": "string"
                },
                "age": {
                    "type": "string"
                },
                "date_of_birth": {
                    "type": "string"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "admin",
                        "client",
                        "personal_trainer"
                    ]
                },
                "phone": {
                    "type": "string"
                },
                "genre": {
                    "type": "string",
                    "enum": [
                        "male",
                        "female"
                    ]
                },
                "contract_start": {
                    "type": "string"
                },
                "contract_end": {
                    "type": "string"
                }
            },
            "xml": {
                "name": "User"
            }
        },
        "Address": {
            "type": "object",
            "required": [
                "id",
                "created_at",
                "user_id",
                "zip_code",
                "number"
